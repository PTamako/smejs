  height: 500px;
}
.top {
  width: 100%;
  height: 69px;
  background-color: rgba(255, 255, 255, 0.1);
  box-sizing: border-box;
  position: absolute;
  top: 125px;
  left: 0;
  z-index: 9999999;
  margin-top: -3px;
  border-bottom: 1px solid rgba(255, 255, 255, 0.5);
}
.top .w {
